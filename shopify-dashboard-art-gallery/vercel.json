{
  "version": 2,
  "builds": [
    {
      "src": "dist/Art-Gallery/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/Art-Gallery"
      }
    },
    {
      "src": "dist/src-dashboard/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/src-dashboard"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Art-Gallery/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/src-dashboard/$1"
    }
  ]
}
